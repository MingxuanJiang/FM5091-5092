        <TextBox x:Name="TextBox_Delta" HorizontalAlignment="Left" Height="23" Margin="518,137,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="191"/>
        <TextBox x:Name="TextBox_Gamma" HorizontalAlignment="Left" Height="23" Margin="518,167,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="191"/>
        <TextBox x:Name="TextBox_Vega" HorizontalAlignment="Left" Height="23" Margin="518,199,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="191"/>
        <TextBox x:Name="TextBox_Theta" HorizontalAlignment="Left" Height="23" Margin="518,227,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="191"/>
        <TextBox x:Name="TextBox_Rho" HorizontalAlignment="Left" Height="23" Margin="518,259,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="191"/>
        <TextBox x:Name="TextBox_Std" HorizontalAlignment="Left" Height="23" Margin="518,289,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="191"/>
    </Grid>
</Window>
